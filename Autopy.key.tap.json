{
    "pinch" : "autopy.key.tap(autopy.key.Code.SPACE)",
    "right" : "autopy.key.tap(autopy.key.Code.RIGHT_ARROW,modifiers=[autopy.key.Modifier.CONTROL])",
    "left" : "autopy.key.tap(autopy.key.Code.LEFT_ARROW,modifiers=[autopy.key.Modifier.CONTROL])",
    "up" :   "autopy.key.tap(autopy.key.Code.UP_ARROW,modifiers=[autopy.key.Modifier.CONTROL])",
    "down" : "autopy.key.tap(autopy.key.Code.DOWN_ARROW,modifiers=[autopy.key.Modifier.CONTROL])",
    "click" : "autopy.mouse.click()"
}